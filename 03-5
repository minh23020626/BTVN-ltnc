#include<bits/stdc++.h>
using namespace std;
#define MIN 1e9
#define MAX -1e9
#define ll long long
#define pb push_back
int main(){
    string s;
    int n;cin >> n;
    s="hackerrank";
    for(int i =0;i<n;i++){
        string a; cin >> a;
        int k = 0;
        for(int j=0;j<(int)a.size();j++){
            if(a[j]==s[k]){
                k++;
            }
            if(k==9) break;
        }
        if(k==9){
            cout << "YES" << endl;
        }
        else cout << "NO" << endl;
    }

}
