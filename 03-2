#include<bits/stdc++.h>
using namespace std;
#define MIN 1e9
#define MAX -1e9
#define ll long long
#define pb push_back
int main(){
    int n; cin >> n;
    int k = n-1;
    for(int i =1 ;i<=n;i++){
        for(int j = 1; j <= n;j++){
            if(j<=k) cout << " ";
            else cout <<"#";
        }
        k--;
        cout << endl;
    }
}
